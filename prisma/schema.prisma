// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
  // provider = "mysql"
  // url      = env("DATABASE_URL")

}

model User {
  id       String  @id @default(cuid())
  fname    String?
  lname    String?
  email    String  @unique
  image    String?
  password String
}
